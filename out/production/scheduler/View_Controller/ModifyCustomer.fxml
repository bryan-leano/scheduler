<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.ModifyCustomerController">

    <children>
        <VBox layoutX="120.0" layoutY="102.0" prefHeight="420.0" prefWidth="360.0">
            <children>
                <GridPane vgap="30.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="176.0" minWidth="10.0" prefWidth="136.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="254.0" minWidth="10.0" prefWidth="224.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Customer ID" />
                        <Label text="Customer Name" GridPane.rowIndex="1" />
                        <Label text="Phone Number" GridPane.rowIndex="2" />
                        <Label text="Address" GridPane.rowIndex="3" />
                        <Label text="Country" GridPane.rowIndex="4" />
                        <Label text="City" GridPane.rowIndex="5" />
                        <Label text="Zip Code" GridPane.rowIndex="6" />

                        <TextField fx:id="custIdTxt" GridPane.columnIndex="1" />
                        <TextField fx:id="nameTxt" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <TextField fx:id="phoneTxt" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <TextField fx:id="addressTxt" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <ComboBox fx:id="countryComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <ComboBox fx:id="cityComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <TextField fx:id="zipTxt" GridPane.columnIndex="1" GridPane.rowIndex="6" />


                    </children>
                </GridPane>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Button mnemonicParsing="false" text="Save" />
                        <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="Cancel" />
                    </children>
                </HBox>
            </children>

        </VBox>
        <DialogPane contentText="MODIFY CUSTOMER" layoutX="230.0" layoutY="14.0" prefHeight="57.0" prefWidth="141.0" />
    </children>
</AnchorPane>
